{
  "name": "expo-meta-wearables-dat",
  "version": "0.0.6",
  "description": "This package enables developers to utilize Meta's AI glasses (Meta Wearables DAT) in Expo + React Native applications.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "react-native": "./build/index.js",
      "default": "./build/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "build",
    "src",
    "ios/*.podspec",
    "ios/*.swift",
    "android",
    "plugin/build",
    "plugin/package.json",
    "app.plugin.js",
    "expo-module.config.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "husky && expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-meta-wearables-dat",
    "EMWDAT",
    "meta",
    "wearables",
    "glasses",
    "ray-ban",
    "camera",
    "streaming",
    "ios",
    "bluetooth",
    "dat"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circus-kitchens/expo-meta-wearables-dat.git"
  },
  "bugs": {
    "url": "https://github.com/circus-kitchens/expo-meta-wearables-dat/issues"
  },
  "author": "Circus Group <fabricio_ext@circuskitchens.com> (https://github.com/circus-kitchens/)",
  "license": "MIT",
  "homepage": "https://github.com/circus-kitchens/expo-meta-wearables-dat#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@commitlint/types": "^20.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.5",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tsconfig/node18": "^18.2.6",
    "@types/node": "^25.2.3",
    "@types/react": "~19.1.0",
    "eslint": "^9.39.2",
    "eslint-config-universe": "^15.0.3",
    "expo": "^54.0.27",
    "expo-module-scripts": "^5.0.8",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react-native": "0.81.5",
    "semantic-release": "^25.0.3",
    "@semantic-release/npm": "^12.0.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24"
  },
  "peerDependencies": {
    "expo": ">=52",
    "react": ">=18",
    "react-native": ">=0.76"
  },
  "packageManager": "pnpm@10.29.2",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  }
}
